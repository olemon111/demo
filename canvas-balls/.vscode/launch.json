{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome against localhost",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceRoot}",
			"file": "${workspaceRoot}/crashBalls/index.html"
		},
		{
			"type": "chrome",
			"request": "attach",
			"name": "Attach to Chrome",
			"port": 9222,
			"webRoot": "${workspaceRoot}"
		},
		{
			"name": "debug index",
			"type": "chrome",
			"request": "launch",
			"sourceMaps": false,
			"file": "${workspaceRoot}/test/index.html"
		}
	]
}
